		}
		.example2 rect#antena1 {
			fill:rgba(204, 204, 0, 1);
			-webkit-transform: rotateZ(10deg);
		}
		.example2 rect#antena2 {
			fill:rgba(204, 204, 0, 1);
			-webkit-transform: rotateZ(-10deg);
		}

		.example2 circle{
			fill:rgba(255, 255, 255, 1);
		}
		.example2 ellipse{
			fill:rgba(204, 204, 0, 1);
			/*stroke-width:2;
			stroke:rgba(184, 184, 0, 1);*/
		}
		.example2 line{
			stroke-width:2;
			stroke:rgba(0, 204, 0, 1);
		}
